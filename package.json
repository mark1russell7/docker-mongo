{
  "$schema": "https://json.schemastore.org/package",
  "name": "docker-mongo",
  "version": "0.1.0",
  "description": "Reusable MongoDB Docker setup with TypeScript utilities for init and seed operations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "docker-compose.yml",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "mongodb": "^6.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "mongodb",
    "docker",
    "docker-compose",
    "database",
    "seed",
    "init",
    "typescript"
  ],
  "license": "MIT"
}
